{
  "name": "3dfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "deploy": "gh-pages --repo https://github.com/Axlisa/my-portfolio.git -d dist",
  "scripts": {
    "dev": "npm run dev:concurrent",
    "dev:frontend": "vite",
    "dev:backend": "bun --watch run index.ts",
    "dev:concurrent": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "test": "npm run test:concurrent",
    "test:server": "bun --watch run test-server.ts",
    "test:concurrent": "concurrently \"npm run test:server\" \"npm run dev:frontend\"",
    "build": "vite build",
    "preview": "vite preview",
    "start": "bun run index.ts",
    "build:full": "npm run build && npm run start",
    "build:github": "GITHUB_ACTIONS=true vite build",
    "deploy:github": "npm run build:github && gh-pages -d dist",
    "deploy:vercel": "vercel"
  },
  "dependencies": {
    "@arcjet/bun": "^1.0.0-beta.7",
    "@arcjet/inspect": "^1.0.0-beta.7",
    "@emailjs/browser": "^3.10.0",
    "@gsap/react": "^2.1.2",
    "@heroicons/react": "^2.2.0",
    "@react-three/drei": "^9.56.24",
    "@react-three/fiber": "^8.11.1",
    "email-js": "^2.0.3",
    "framer-motion": "^9.0.7",
    "gsap": "^3.12.7",
    "maath": "^0.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.4.0",
    "react-router-dom": "^6.8.1",
    "react-tilt": "^1.0.2",
    "react-vertical-timeline-component": "^3.6.0",
    "three": "^0.172.0",
    "vanilla-tilt": "^1.8.1",
    "vercel": "^25.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.13",
    "concurrently": "^8.2.2",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.6",
    "vite": "^6.3.5"
  }
}
